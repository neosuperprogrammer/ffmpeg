<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android"
>
	﻿
	<item
		android:state_window_focused="false"
		android:state_enabled="true"
		android:drawable="@drawable/btn_pause" /> <!-- normal -->
	﻿
	<item
		android:state_window_focused="false"
		android:state_enabled="false"
		android:drawable="@drawable/btn_pause" /> <!-- disable -->
	<item
		android:state_pressed="true"
		android:drawable="@drawable/btn_pause_tou" /> <!-- press -->
	<item
		android:state_focused="true"
		android:state_enabled="true"
		android:drawable="@drawable/btn_pause_tou" /> <!-- focus -->
	<item
		android:state_enabled="true"
		android:drawable="@drawable/btn_pause" /> <!-- normal -->
	<item
		android:state_focused="true"
		android:drawable="@drawable/btn_pause" /> <!-- disable focus -->
	<item
		android:drawable="@drawable/btn_pause" /> <!-- disable -->
</selector>